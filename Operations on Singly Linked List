#include <stdio.h>
#include <stdlib.h>

struct node {
    int value;
    struct node *next;
};
struct node *head;
void insert(){
struct node *temp;
struct node*newnode=(struct node*)malloc(sizeof(struct node));
{
printf("Element: ")&&scanf("%d",&newnode->value);
newnode->next=NULL;
if(head==NULL){
head=newnode;
}
else{
temp=head;
while(temp->next!=NULL){
temp=temp->next;
}
temp->next=newnode;
}
}
}
